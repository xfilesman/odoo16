                        <field name="col26" style="white-space: normal; zoom: 0.9;"/>
                        <field name="col27" style="white-space: normal; zoom: 0.9;"/>
                    </group>
                </sheet>
                <footer>
                    <button name="action_apply" type="object" string="Apply" class="btn-primary"/>
